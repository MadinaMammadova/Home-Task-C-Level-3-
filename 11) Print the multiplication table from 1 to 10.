#include <iostream>
using namespace std;
int main() {
    int i=1;
for(i;i<=10;i++) {
    cout<<1<<"*"<<i<<"="<<i*1<<endl;
    cout<<2<<"*"<<i<<"="<<i*2<<endl;
    cout<<3<<"*"<<i<<"="<<i*3<<endl;
    cout<<4<<"*"<<i<<"="<<i*4<<endl;
    cout<<5<<"*"<<i<<"="<<i*5<<endl;
    cout<<6<<"*"<<i<<"="<<i*6<<endl;
    cout<<7<<"*"<<i<<"="<<i*7<<endl;
    cout<<8<<"*"<<i<<"="<<i*8<<endl;
    cout<<9<<"*"<<i<<"="<<i*9<<endl;
    cout<<10<<"*"<<i<<"="<<i*10<<endl;
    cout<<endl;
}
    return 0;
}
